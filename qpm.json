{
  "$schema": "https://raw.githubusercontent.com/QuestPackageManager/QPM.Package/refs/heads/main/qpm.schema.json",
  "version": "0.4.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "BSML",
    "id": "bsml",
    "version": "0.5.0",
    "url": "https://github.com/bsq-ports/Quest-BSML",
    "additionalData": {
      "overrideSoName": "libbsml.so",
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1"
      ],
      "log": [
        "pwsh ./scripts/log.ps1"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1",
        "qpm qmod manifest",
        "pwsh ./scripts/createqmod.ps1 BSML -clean"
      ]
    },
    "ndk": "^27.2.12479018",
    "qmodIncludeDirs": [
      "build",
      "extern/libs"
    ],
    "qmodIncludeFiles": [],
    "qmodOutput": "./BSML.qmod"
  },
  "dependencies": [
    {
      "id": "libil2cpp",
      "versionRange": "^0.4.0",
      "additionalData": {}
    },
    {
      "id": "beatsaber-hook",
      "versionRange": "^6.4.1",
      "additionalData": {}
    },
    {
      "id": "bs-cordl",
      "versionRange": "^4008.0.0",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.18.2",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.6",
      "additionalData": {
        "includeQmod": false,
        "private": true
      }
    },
    {
      "id": "paper2_scotland2",
      "versionRange": "^4.6.1",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "tinyxml2",
      "versionRange": "^10.0.0",
      "additionalData": {}
    },
    {
      "id": "gif-lib",
      "versionRange": "^5.2.2",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "kaleb",
      "versionRange": "^0.1.9",
      "additionalData": {
        "private": true
      }
    }
  ]
}
